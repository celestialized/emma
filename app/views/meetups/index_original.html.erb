


<div class="row">
  <div class="col-sm-6">
    <h1>Meetups</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%#= link_to new_meetup_path, class: 'btn btn-primary' do %>
    <%# Add New Meetup %>
  <%# end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Title</th>
            <th>Start date</th>
            <th>End date</th>
            <th>More info url</th>
            <th>Location</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @meetups.each do |meetup| %>
        <%= content_tag :tr, id: dom_id(meetup), class: dom_class(meetup) do %>
          

                      <td><%= meetup.title %></td>
                      <td><%= meetup.start_date %></td>
                      <td><%= meetup.end_date %></td>
                      <td><%= meetup.more_info_url %></td>
                      <td><%= meetup.location %></td>
          
          
            <td><%= link_to 'Show', meetup %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
